
<h4>Login</h4>
<form action="<%= login_path %>" method="post" class="form">
        <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" />
    <div class="form-group">
        <input type="text" name="user[email]" class="form-control" id="email" placeholder="Email" />
    </div>
    <div class="form-group">
        <input type="password" name="user[password]" class="form-contol" id="password" placeholder="Password" />
    </div>
    <div class="form-group">
        <button class="btn btn-primary" type="submit">Login</button>
    </div>
</form>
